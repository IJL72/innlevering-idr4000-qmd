@article{del_coso_more_2019,
	title = {More than a ‘speed gene’: {ACTN3} {R577X} genotype, trainability, muscle damage, and the risk for injuries},
	volume = {119},
	issn = {1439-6327},
	shorttitle = {More than a ‘speed gene’},
	url = {https://doi.org/10.1007/s00421-018-4010-0},
	doi = {10.1007/s00421-018-4010-0},
	abstract = {A common null polymorphism (rs1815739; R577X) in the gene that codes for α-actinin-3 (ACTN3) has been related to different aspects of exercise performance. Individuals who are homozygous for the X allele are unable to express the α-actinin-3 protein in the muscle as opposed to those with the RX or RR genotype. α-actinin-3 deficiency in the muscle does not result in any disease. However, the different ACTN3 genotypes can modify the functioning of skeletal muscle during exercise through structural, metabolic or signaling changes, as shown in both humans and in the mouse model. Specifically, the ACTN3 RR genotype might favor the ability to generate powerful and forceful muscle contractions. Leading to an overall advantage of the RR genotype for enhanced performance in some speed and power-oriented sports. In addition, RR genotype might also favor the ability to withstand exercise-induced muscle damage, while the beneficial influence of the XX genotype on aerobic exercise performance needs to be validated in human studies. More information is required to unveil the association of ACTN3 genotype with trainability and injury risk during acute or chronic exercise.},
	language = {en},
	number = {1},
	urldate = {2022-11-30},
	journal = {European Journal of Applied Physiology},
	author = {Del Coso, Juan and Hiam, Danielle and Houweling, Peter and Pérez, Laura M. and Eynon, Nir and Lucía, Alejandro},
	month = jan,
	year = {2019},
	keywords = {Athletic performance, Common human polymorphism, Genomics, Muscle performance, α-Actinin-3 deficiency},
	pages = {49--60},
	file = {Full Text PDF:/Users/ingerjohannelokkevik/Zotero/storage/KSTJ75BZ/Del Coso et al. - 2019 - More than a ‘speed gene’ ACTN3 R577X genotype, tr.pdf:application/pdf},
